{
  "manifest_version": 3,
  "name": "YT sin galletas",
  "version": "1.0",
  "description": "Open YT video without ADS",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "author": "Imanol Santiago",
  "homepage_url": "https://github.com/ImanolSantiago/YT-sin-galletas",
  "license": "Apache-2.0"
}
